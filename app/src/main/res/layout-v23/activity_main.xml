<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/welcomePage"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:fadingEdge="horizontal|vertical"    android:foreground="@android:color/transparent"    tools:context=".MainActivity">    <ImageView        android:id="@+id/imageView3"        android:layout_width="416dp"        android:layout_height="66dp"        android:contentDescription="TODO"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:srcCompat="@android:drawable/screen_background_dark" />    <TextView        android:id="@+id/welcomeText"        android:layout_width="395dp"        android:layout_height="wrap_content"        android:layout_marginTop="24dp"        android:singleLine="false"        android:soundEffectsEnabled="false"        android:text="@string/welcome_to_opennac_authentication_service"        android:textAlignment="center"        android:textAllCaps="false"        android:textAppearance="@style/TextAppearance.AppCompat.Display1"        android:textColor="#000000"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.209" />    <Button        android:id="@+id/beginButton"        android:layout_width="160dp"        android:layout_height="45dp"        android:layout_marginBottom="24dp"        android:background="@color/azure"        android:backgroundTint="@color/clickedColor"        android:soundEffectsEnabled="false"        android:text="@string/click_here_to_begin"        android:textColor="#FFFFFF"        android:textStyle="bold"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.497"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.932" />    <Button        android:id="@+id/infoBtn"        android:layout_width="160dp"        android:layout_height="54dp"        android:background="#E1000000"        android:text="@string/visit_our_website"        android:textColor="#FFFFFF"        android:textStyle="bold"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.498"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.435" />    <ImageButton        android:id="@+id/spanishlang"        android:layout_width="186dp"        android:layout_height="142dp"        android:adjustViewBounds="true"        android:contentDescription="@string/espa_ol"        android:hapticFeedbackEnabled="true"        android:scaleType="fitXY"        android:soundEffectsEnabled="true"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.035"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.724"        app:srcCompat="@drawable/spain_flag" />    <ImageButton        android:id="@+id/englang"        android:layout_width="183dp"        android:layout_height="142dp"        android:layout_marginEnd="16dp"        android:adjustViewBounds="true"        android:contentDescription="@string/english"        android:hapticFeedbackEnabled="true"        android:scaleType="fitXY"        android:soundEffectsEnabled="true"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="1.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.722"        app:srcCompat="@drawable/uk_flag" />    <ImageView        android:id="@+id/appLogoLight"        android:layout_width="172dp"        android:layout_height="57dp"        app:layout_constraintBottom_toBottomOf="@+id/imageView3"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="@+id/imageView3"        app:layout_constraintVertical_bias="1.0"        app:srcCompat="@drawable/applogowhite" /></androidx.constraintlayout.widget.ConstraintLayout>