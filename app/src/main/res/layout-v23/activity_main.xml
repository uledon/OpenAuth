<?xml version="1.0" encoding="utf-8"?><androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/drawer_layout"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:fitsSystemWindows="true"    tools:context=".MainActivity"    tools:openDrawer="start">    <com.google.android.material.navigation.NavigationView        android:id="@+id/nav_view"        android:layout_width="wrap_content"        android:layout_height="match_parent"        android:layout_gravity="start"        android:background="@color/greyback"        android:fontFamily="@font/fira_sans"        app:itemTextColor="@color/colorPrimaryDark"        app:menu="@menu/main_menu"        tools:visibility="gone" />    <androidx.constraintlayout.widget.ConstraintLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="@color/greyback">        <include            android:id="@+id/toolbar"            layout="@layout/toolbar"/>        <ImageView            android:id="@+id/bottomBack"            android:layout_width="0dp"            android:layout_height="0dp"            android:layout_marginStart="10dp"            android:layout_marginTop="32dp"            android:layout_marginEnd="10dp"            android:scaleType="fitXY"            android:tint="@color/white"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0.0"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="@+id/appLogo"            app:layout_constraintVertical_bias="0.0"            app:srcCompat="@drawable/bottomrear" />        <Button            android:id="@+id/beginButton"            android:layout_width="160dp"            android:layout_height="100dp"            android:layout_marginBottom="196dp"            android:background="@drawable/basicbutton"            android:backgroundTint="@color/colorPrimary"            android:fontFamily="@font/fira_sans"            android:soundEffectsEnabled="false"            android:text="@string/connect"            android:textColor="#FFFFFF"            android:textStyle="bold"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="@+id/bottomBack"            app:layout_constraintVertical_bias="0.989" />        <Button            android:id="@+id/infoBtn"            android:layout_width="160dp"            android:layout_height="100dp"            android:layout_marginStart="24dp"            android:layout_marginEnd="5dp"            android:layout_marginBottom="64dp"            android:background="@drawable/basicbutton"            android:backgroundTint="@color/colorPrimaryDark"            android:fontFamily="@font/fira_sans"            android:text="@string/visit_our_website"            android:textColor="#FFFFFF"            android:textStyle="bold"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@+id/helpBtn"            app:layout_constraintHorizontal_bias="0.0"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="@+id/bottomBack"            app:layout_constraintVertical_bias="0.995" />        <Button            android:id="@+id/helpBtn"            android:layout_width="160dp"            android:layout_height="100dp"            android:layout_marginStart="5dp"            android:layout_marginEnd="24dp"            android:layout_marginBottom="64dp"            android:background="@drawable/basicbutton"            android:backgroundTint="@color/colorAccent"            android:clickable="true"            android:focusable="true"            android:fontFamily="@font/fira_sans"            android:text="@string/help"            android:textColor="#FFFFFF"            android:textStyle="bold"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toEndOf="@id/infoBtn"            app:layout_constraintTop_toTopOf="@+id/bottomBack"            app:layout_constraintVertical_bias="0.992" />        <ImageView            android:id="@+id/bigBack2"            android:layout_width="0dp"            android:layout_height="0dp"            android:scaleType="fitXY"            android:tint="@color/colorPrimary"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:srcCompat="@drawable/bigbackground" />        <ImageView            android:id="@+id/appLogo"            android:layout_width="166dp"            android:layout_height="206dp"            android:gravity="center"            app:layout_constraintBottom_toBottomOf="@+id/bigBack2"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0.50"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:srcCompat="@drawable/opencloudfactorylogo" />        <TextView            android:id="@+id/welcomeText"            android:layout_width="397dp"            android:layout_height="48dp"            android:layout_marginTop="24dp"            android:fontFamily="@font/fira_sans"            android:singleLine="false"            android:soundEffectsEnabled="false"            android:text="@string/welcome"            android:textAlignment="center"            android:textAllCaps="false"            android:textAppearance="@style/TextAppearance.AppCompat.Display1"            android:textColor="#FFF8F8"            android:textStyle="bold"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:layout_constraintVertical_bias="0.08" />    </androidx.constraintlayout.widget.ConstraintLayout></androidx.drawerlayout.widget.DrawerLayout>